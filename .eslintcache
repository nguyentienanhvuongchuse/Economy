tegory: {category?.title}</h1>\n      <h2>Category Products</h2>\n      <div className=\"row\">\n      {\n        category !== null &&\n        category?.category_product?.map((product,index) =>(\n          <div className=\"col-md-3\">\n            <SingleProduct item={product} key={index}/>\n          </div>\n        ))\n      }\n      </div>\n    </div>\n  )\n}\n","/home/techcare/Desktop/shop-api/src/components/ProfilePage.jsx",["85"],"import Axios from 'axios'\nimport React, { useState } from 'react'\nimport { domain, header } from '../env'\nimport { useGlobalState } from '../state/provider'\n\n\nexport const ProfilePage = () => {\n  const [{ profile }, dispatch] = useGlobalState()\n  const [email, setemail] = useState(profile?.prouser?.email)\n  const [lastname, setlastname] = useState(profile?.prouser?.last_name)\n  const [firstname, setfirstname] = useState(profile?.prouser?.first_name)\n  const [image, setimage] = useState(null)\n\n  const updateprofile = async () => {\n    await Axios({\n      method: 'POST',\n      url: `${ domain}/api/updateprofile/`,\n      headers: header,\n      data: {\n        'first_name':firstname,\n        'last_name':lastname,\n        'email':email\n      }\n    }).then(response => {\n      dispatch({\n        type: \"PAGE_RELOAD\",\n        pagereload: response.data\n      })\n    })\n  }\n\n  const updateimg = async () => {\n    const fromdata = new FormData()\n    fromdata.append('image', image)\n    await Axios({\n      method: 'POST',\n      url: `${ domain}/api/updateimg/`,\n      headers: header,\n      data: fromdata\n    }).then(response => {\n      dispatch({\n        type: \"PAGE_RELOAD\",\n        pagereload: response.data\n      })\n    })\n  }\n\n  return (\n    <div className=\"container\">\n      <div className=\"row\">\n        <div className=\"col-md-4\">\n          <div className=\"media\">\n            <img src={ `${domain}${profile?.image}`} className=\"account-img\"/>\n            <div className=\"media-body\">\n              <h2>{profile?.prouser?.username}</h2>\n            </div>\n          </div>\n        </div>\n        <div className=\"col-md-8\">\n          <p>Name:     {profile?.prouser?.last_name} {profile?.prouser?.first_name}</p>\n          <p>Email:    {profile?.prouser?.email}</p>\n        </div>\n      </div>\n      <div className=\"\">\n        <div className=\"form-group\">\n          <label>Image</label>\n          <input onChange={e => setimage(e.target.files[0])} type=\"file\" className=\"form-control\"/>\n        </div>\n          <button onClick={updateimg} className=\"btn btn-info my-2\">Upload</button>\n        <div className=\"form-group\">\n          <label>Email</label>\n          <input onChange={(e) => setemail(e.target.value)} type=\"email\" className=\"form-control\" value={email}/>\n        </div>\n        <div className=\"form-group\">\n          <label>Firstname</label>\n          <input onChange={(e) => setfirstname(e.target.value)} type=\"text\" className=\"form-control\" value={firstname}/>\n        </div>\n        <div className=\"form-group\">\n          <label>Lastname</label>\n          <input onChange={(e) => setlastname(e.target.value)} type=\"text\" className=\"form-control\" value={lastname}/>\n        </div>\n        <button onClick={updateprofile} className=\"btn btn-success my-2\">Update</button>\n      </div>\n    </div>\n  )\n}\n","/home/techcare/Desktop/shop-api/src/components/LoginPage.jsx",[],"/home/techcare/Desktop/shop-api/src/components/RegisterPage.jsx",[],"/home/techcare/Desktop/shop-api/src/state/provider.js",[],"/home/techcare/Desktop/shop-api/src/state/reducer.js",[],"/home/techcare/Desktop/shop-api/src/components/Cart.jsx",["86"],"/home/techcare/Desktop/shop-api/src/components/OldOrders.jsx",["87"],"/home/techcare/Desktop/shop-api/src/components/Order.jsx",[],"/home/techcare/Desktop/shop-api/src/components/OrderDetails.jsx",["88"],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"